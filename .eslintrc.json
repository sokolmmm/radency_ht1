{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "max-len": [
      "warn",
      {
        "code": 120,
        "tabWidth": 2,
        "comments": 200,
        "ignoreUrls": true,
        "ignoreStrings": false
      }
    ]
  }
}
